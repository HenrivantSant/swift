    "php8",
    "rest",
    "rest-api",
    "graphql-api",
    "graphql-server",
    "cli-app"
  ],
  "homepage": "https://github.com/SwiftAPI/swift",
  "license": "MIT",
  "authors": [
    {
      "name": "Henri van t Sant",
