    "cycle/entity-behavior-uuid": "^1.2.0",
    "psr/http-server-middleware": "^1.0.2"
  },
  "require-dev": {
    "symfony/maker-bundle": "^v1.59.1",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Swift\\": "src"
    }
