    "phpdocumentor/reflection-docblock": "^5.4.0",
    "guzzlehttp/guzzle": "^7.8.1",
    "symfony/mime": "^v7.0.7",
    "symfony/rate-limiter": "^v7.0.7",
    "symfony/config": "^v7.0.7",
    "symfony/dependency-injection": "^v7.0.7",
    "symfony/yaml": "^v7.0.7",
